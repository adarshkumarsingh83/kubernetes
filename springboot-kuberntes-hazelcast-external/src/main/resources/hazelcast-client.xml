<hazelcast-client xsi:schemaLocation="http://www.hazelcast.com/schema/client-config
                http://www.hazelcast.com/schema/hazelcast-client-config.xsd"
                  xmlns="http://www.hazelcast.com/schema/client-config"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

    <group>
        <name>imdg</name>
        <password>imdg</password>
    </group>

    <!-- IMDG group port 8001. Jet group port 9001 -->
    <network>
        <cluster-members>
            <address>127.0.0.1:5701</address>
        </cluster-members>
    </network>

</hazelcast-client>